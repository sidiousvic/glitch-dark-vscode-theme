{
  "tokenColors": [
    {
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "Preprocessor Statements",
      "scope": ["meta.preprocessor", "keyword.control.import"],
      "settings": {
        "foreground": "#8400ff"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e00909"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#e90ac0"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#0004ff"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.language", "variable.other"],
      "settings": {
        "foreground": "#e90ac0"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#00ff0d"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#e90ac0"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#00ff0d"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff06c9"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#8400ff"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#8400ff"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#8400ff"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#8400ff"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#8400ff"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {}
    }
  ],
  /*!
